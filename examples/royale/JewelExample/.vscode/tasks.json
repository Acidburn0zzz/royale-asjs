{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "type": "actionscript",
            "debug": true,
            "problemMatcher": [
                "$nextgenas_nomatch"
            ],
            "group": "build"
        },
        {
            "label": "Build with Maven",
            "type": "shell",
            "command": "mvn",
            "args": [
                "clean",
                "install",
                "-DskipTests"
            ],
            "group": "build",
            "presentation": {
                "reveal": "always",
                "panel": "new"
            }
        }
    ]
    
}